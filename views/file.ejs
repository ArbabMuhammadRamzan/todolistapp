<%- include("header") -%>
  <div class="box" id="heading">
    <h1>
      <%= head %>
    </h1>
  </div>
  <div class="box">
    <% for(var x = 0; x < liItem.length; x++){ %>

    <div class="item">
      <input type="checkbox">
      <p> <%= liItem[x] %> </p>
    </div>

    <% } %>
    <form class="item" action="/" method="post" class="item">
      <input type="text" name="valueSend" placeholder="New Item" autocomplete="off"><br>
      <button type="submit" name="item" value="<%= head %>">+</button>
    </form>
  </div>
<%- include("footer") -%>
